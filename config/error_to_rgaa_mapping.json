  "fallback_patterns": [
    {
      "pattern": "rgaa[_\\s-]*(\\d+)[._-](\\d+)",
      "description": "Extract RGAA number from test name (e.g., 'rgaa-1-1' or 'RGAA 1.1')"
    },
    {
      "keywords": ["image", "img", "alt"],
      "default_criteria": "1.1",
      "description": "Image-related errors default to RGAA 1.1"
    },
    {
      "keywords": ["contrast", "color"],
      "default_criteria": "3.2",
      "description": "Contrast errors default to RGAA 3.2"
    },
    {
      "keywords": ["form", "label", "input", "select"],
      "default_criteria": "11.1",
      "description": "Form errors default to RGAA 11.1"
    },
    {
      "keywords": ["button"],
      "default_criteria": "7.1",
      "description": "Button errors default to RGAA 7.1"
    },
    {
      "keywords": ["link", "lien"],
      "default_criteria": "6.1",
      "description": "Link errors default to RGAA 6.1"
    },
    {
      "keywords": ["heading", "titre", "h1", "h2"],
      "default_criteria": "9.1",
      "description": "Heading errors default to RGAA 9.1"
    },
    {
      "keywords": ["focus", "keyboard", "clavier"],
      "default_criteria": "7.3",
      "description": "Keyboard/focus errors default to RGAA 7.3"
    }
  ]
